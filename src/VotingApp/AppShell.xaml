<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="VotingApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:VotingApp"
    Shell.FlyoutBehavior="Disabled"
    Title="VotingApp">

    <ShellContent
        Title="VotingApp"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="VotingApp"
        ContentTemplate="{DataTemplate local:CreatePage}"
        Route="CreatePage" />
    <ShellContent
        Title="VotingApp"
        ContentTemplate="{DataTemplate local:SurveyPage}"
        Route="SurveyPage" />
</Shell>
